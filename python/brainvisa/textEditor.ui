<!DOCTYPE UI><UI version="3.3" stdsetdef="1">
<class>TextEditor</class>
<widget class="QDialog">
    <property name="name">
        <cstring>TextEditor</cstring>
    </property>
    <property name="geometry">
        <rect>
            <x>0</x>
            <y>0</y>
            <width>601</width>
            <height>480</height>
        </rect>
    </property>
    <property name="caption">
        <string>Text editor</string>
    </property>
    <vbox>
        <property name="name">
            <cstring>unnamed</cstring>
        </property>
        <widget class="QLabel">
            <property name="name">
                <cstring>BV_info</cstring>
            </property>
            <property name="text">
                <string></string>
            </property>
        </widget>
        <widget class="QTextEdit">
            <property name="name">
                <cstring>BV_content</cstring>
            </property>
            <property name="wordWrap">
                <enum>NoWrap</enum>
            </property>
        </widget>
        <widget class="QLayoutWidget">
            <property name="name">
                <cstring>layout3</cstring>
            </property>
            <hbox>
                <property name="name">
                    <cstring>unnamed</cstring>
                </property>
                <widget class="QLabel">
                    <property name="name">
                        <cstring>BV_cursorPosition</cstring>
                    </property>
                    <property name="text">
                        <string></string>
                    </property>
                </widget>
                <spacer>
                    <property name="name">
                        <cstring>spacer1</cstring>
                    </property>
                    <property name="orientation">
                        <enum>Horizontal</enum>
                    </property>
                    <property name="sizeType">
                        <enum>Expanding</enum>
                    </property>
                    <property name="sizeHint">
                        <size>
                            <width>350</width>
                            <height>20</height>
                        </size>
                    </property>
                </spacer>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>BV_btnOk</cstring>
                    </property>
                    <property name="text">
                        <string>Ok</string>
                    </property>
                </widget>
                <widget class="QPushButton">
                    <property name="name">
                        <cstring>BV_btnCancel</cstring>
                    </property>
                    <property name="text">
                        <string>Cancel</string>
                    </property>
                </widget>
            </hbox>
        </widget>
    </vbox>
</widget>
<connections>
    <connection>
        <sender>BV_btnOk</sender>
        <signal>clicked()</signal>
        <receiver>TextEditor</receiver>
        <slot>accept()</slot>
    </connection>
    <connection>
        <sender>BV_btnCancel</sender>
        <signal>clicked()</signal>
        <receiver>TextEditor</receiver>
        <slot>reject()</slot>
    </connection>
</connections>
<layoutdefaults spacing="6" margin="11"/>
</UI>
